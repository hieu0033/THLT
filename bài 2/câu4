print("mssv:235752021610033")
a = int(input("enter the start of the range (a): "))
b = int(input("enter the end of the range (b): "))
for i in range(a + 1, b):
    for j in range(2, 10):
        if i !=0:
            print("i:", i, "j:", j)
            print("nghịch đảo của", i, "/",j, "=", i / j)
        else:
            print("i bằng 0, không thể tích nghịch đảo.")
       
