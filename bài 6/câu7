print('bui van hieu')
print('235752021610033')
class Circle:
    def __init__(self, radius):
        self.radius = radius

    def area(self):
        return 3.14 * self.radius**2

    def circumference(self):
        return 2 * 3.14 * self.radius
circle = Circle(5)
print("Diện tích:", circle.area())

print("Chu vi:", circle.circumference())
