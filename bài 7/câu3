print("họ và tên :bùi văn hiếu")
print("mssv:235752021610033")
def read_entire_file(filename):
    with open(filename, 'r', encoding='utf-8') as file:
        content = file.read()
        return content
filename = input("Nhập tên tệp cần đọc: ")
file_content = read_entire_file(filename)
print(file_content)
